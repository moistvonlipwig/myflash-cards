<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.teliinc.myflash_cards" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="org.teliinc.myflash_cards.MainActivity" >
        </activity>
        <activity android:name="org.teliinc.myflash_cards.SettingsActivity" >
        </activity>
        <activity android:name="org.teliinc.myflash_cards.LaunchScreenActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.teliinc.myflash_cards.SectionsActivity" >
        </activity>
    </application>

</manifest>
